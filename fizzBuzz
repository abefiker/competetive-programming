class Solution {
    List<String> fizzBuzz = new ArrayList<String>();
    public List<String> fizzBuzz(int n) {
        for(int i = 1; i <= n ; i++) {
		
		  if(i % 3 == 0 && i%5!=0) {
			fizzBuzz.add("Fizz");
		}else if(i%5==0 && i%3 !=0) {
			fizzBuzz.add("Buzz");
		}
		else if(i%3==0 && i%5==0) {
				fizzBuzz.add("FizzBuzz");
			}
		else {
			fizzBuzz.add(Integer.toString(i) );
		}
	}
	
		
		return fizzBuzz;
    }
}
